{
  "name": "my-react",
  "version": "1.0.0",
  "description": "my first react project",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --config ./build/prod.js",
    "dev": "webpack-dev-server --progress --colors --config ./build/dev.js",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run dev",
    "check-types": "npx tsc --skipLibCheck",
    "lint": "npx eslint './src/**/*.ts' './src/**/*.tsx'",
    "lint-fix": "npx eslint './src/**/*.ts' './src/**/*.tsx' --fix",
    "preview": "http-server dist/ -P http://172.16.10.37/"
  },
  "dependencies": {
    "@types/history": "^4.7.6",
    "@types/lodash": "^4.14.157",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.18",
    "@types/whatwg-fetch": "0.0.33",
    "antd": "^3.15.1",
    "history": "^5.0.0",
    "lodash": "^4.17.19",
    "mirror-creator": "^1.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs2": "^7.5.5",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/classnames": "^2.2.7",
    "@types/http-server": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "commitizen": "^4.1.2",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "less": "^3.12.0",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.12.0",
